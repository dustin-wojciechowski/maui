            xmlns:local="clr-namespace:FlyoutProject.Pages"
             x:Class="FlyoutProject.MainPage">


    <FlyoutPage.Flyout>
        <local:MenuPage x:Name="flyoutPage" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local:ContactsPage />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>

    <!--<FlyoutPage.Detail>
